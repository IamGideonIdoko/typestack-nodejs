{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es2022": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
  "parserOptions": { "ecmaVersion": "latest", "sourceType": "module" },
  "plugins": ["unicorn"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "unicorn/filename-case": [
      "error",
      {
        "case": "snakeCase"
      }
    ]
  }
}